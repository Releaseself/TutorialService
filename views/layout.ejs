<% if(typeof(js) !== 'undefined') {
if(Array.isArray(js)) {
        js.forEach(function(item) {
%>
<script type="text/javascript" src="<%= item %>"></script>
<% })} else { %>
<script type="text/javascript" src="<%= js %>"></script>
<% }} %>
